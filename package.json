    "@types/uuid": "^8.3.4",
    "jest": "^28.1.3",
    "json-server": "^0.17.0",
    "ts-jest": "^28.0.7"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
